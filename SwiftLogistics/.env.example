# SwiftTrack Frontend Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=http://localhost:3000

# Development settings
VITE_ENV=development
VITE_DEBUG=true

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000
VITE_WEBSOCKET_TIMEOUT=10000

# Rate limiting
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Cache settings (in minutes)
VITE_CACHE_TTL_DEFAULT=5
VITE_CACHE_TTL_STATIC=60

# Retry configuration
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000

# Feature flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_GEOLOCATION=true
VITE_ENABLE_NOTIFICATIONS=true

# Map configuration (if using maps)
VITE_MAP_API_KEY=your_map_api_key_here
VITE_DEFAULT_MAP_CENTER_LAT=6.9271
VITE_DEFAULT_MAP_CENTER_LNG=79.8612

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_ERROR_REPORTING=true
